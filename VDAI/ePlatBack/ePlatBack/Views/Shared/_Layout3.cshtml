
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/ico" href="/Content/themes/base/images/favicon.ico" />

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous" />

    <!-- Bootstrap Vue -->
    @*<link href="~/Content/plugins/bootstrap-vue/bootstrap-vue.css" rel="stylesheet" />*@
    <link type="text/css" rel="stylesheet" href="//unpkg.com/bootstrap-vue@latest/dist/bootstrap-vue.css" />

    <!-- Bootstrap Multiselect-->
    <link href="~/Content/plugins/bootstrap-multiselect/css/bootstrap-multiselect.css" rel="stylesheet" />

    <!-- Bootstrap DatePicker -->
    <link href="~/Content/plugins/tempus-dominus/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Material Icons-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    <!-- jQuery Confirm-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.0/jquery-confirm.min.css">

    <!-- ePlat Styles -->
    <link href="~/Content/themes/eplat2/layout2.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/themes/eplat2/layout2-printable.css" rel="stylesheet" media="print" type="text/css" />
    <link href="~/Content/themes/eplat2/layout2-screen.css" rel="stylesheet" media="screen" type="text/css" />

    <script src="/Scripts/layout/xhr-targets.js"></script>

    <script src=""></script>
    @RenderSection("Head", required: false)

    <title>@ViewBag.Title</title>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-4043946-52"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-4043946-52');
    </script>

</head>
<body>
    <div id="app">
        <nav class="navbar fixed-top navbar-expand-md navbar-dark bg-dark flex-md-nowrap">
            <a class="navbar-brand" href="/">ePlat</a>

            <div class="collapse navbar-collapse" id="navbarNav">
                <ul id="menu" class="navbar-nav mb-3 mb-sm-0"></ul>
            </div>

            <terminals></terminals>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <i class="material-icons">menu</i>
            </button>
            <div id="secondaryMenu">
                <ul class="navbar-nav">
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="log">
                            <i class="material-icons">assignment</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="email">
                            <i class="material-icons">email</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="sms">
                            <i class="material-icons">sms</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="chat">
                            <i class="material-icons">chat</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="tasks">
                            <i class="material-icons">flag</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="notifications">
                            <i class="material-icons">notifications_none</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="help">
                            <i class="material-icons">help</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="settings">
                            <i class="material-icons">settings</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="#" title="account">
                            <i class="material-icons">account_circle</i>
                        </a>
                    </li>
                    <li class="nav-item text-nowrap d-sm-inline-block">
                        <a class="nav-link eplat-tool" href="/account/logoff" title="Log Off" id="aLogOff">
                            <i class="material-icons">power_settings_new</i>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
        <div class="container-fluid">
            <div class="row">
                <main id="main" role="main" class="col pt-3 px-4">
                    @RenderBody()
                </main>
                <sidebar></sidebar>
            </div>
        </div>
    </div>
    <div id="statusBar" class="non-printable"><span class="message"></span></div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    @*<script src="https://code.jquery.com/jquery-3.2.1.min.js" crossorigin="anonymous"></script>*@

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="~/Content/plugins/jqueryajax/jquery.validate.min.js"></script>
    <script src="~/Content/plugins/jqueryajax/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Content/plugins/jqueryajax/jquery.unobtrusive-ajax.min.js"></script>
    <script src="/Scripts/jquery.json-2.3.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/lodash@4.13.1/lodash.min.js"></script>
    <!-- Vue -->
    <!-- development version, includes helpful console warnings -->
    @*<script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>*@
    <!-- Vuex -->
    @*<script src="https://unpkg.com/vuex@3.0.1/dist/vuex.js"></script>*@
    <!-- production version, optimized for size and speed -->
    @*<script src="https://cdn.jsdelivr.net/npm/vue"></script>*@
    <!-- Bootstrap Vue -->
    @*<script src="~/Content/plugins/bootstrap-vue/bootstrap-vue.js"></script>
        <script src="~/Content/plugins/bootstrap-vue/polyfill.min.js"></script>*@
    <script src="//unpkg.com/babel-polyfill@latest/dist/polyfill.min.js"></script>
    <script src="//unpkg.com/bootstrap-vue@latest/dist/bootstrap-vue.js"></script>
    <!-- Bootstrap Multiselect -->
    <script src="~/Content/plugins/bootstrap-multiselect/js/bootstrap-multiselect.js"></script>
    <!-- Moment -->
    <script src="~/Content/plugins/moment/moment.min.js"></script>
    <!-- DatePicker -->
    <script src="~/Content/plugins/tempus-dominus/tempusdominus-bootstrap-4.min.js"></script>
    <!-- jQuery Confirm -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.0/jquery-confirm.min.js"></script>
    <!-- SignalR-->
    <script src="~/Scripts/jquery.signalR-2.2.1.min.js"></script>
    <script src="~/signalr/hubs"></script>

    <script src="~/ViewModels/Components/utils.js"></script>
    <!--Mover un nivel mas
    <script src="~/ViewModels/eplat.global.filters.js"></script>
    <script src="~/ViewModels/eplat.global.store.js"></script>
    <script src="~/ViewModels/eplat.global.utils.js"></script>
    <script src="~/ViewModels/eplat.global.directives.js"></script>-->
    @RenderSection("Scripts", required: false)
</body>
</html>