<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>@ViewBag.Title</title>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,400,700' rel='stylesheet' type='text/css' />
    <link href="/Content/Site.css?v=20160209" rel="stylesheet" type="text/css" />
    @*<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">*@
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/ui-custom")
    <link href="/Content/Site-printable.css?v=@DateTime.Now" rel="stylesheet" type="text/css" media="print" />
    <link rel="icon" type="image/ico" href="/Content/themes/base/images/favicon.ico" />
    <link href="/Content/plugins/fancybox/jquery.fancybox.css" rel="stylesheet" />
    <link href="~/Content/themes/eplat/jquery.multiselect.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/themes/eplat/jquery.multiselect.filter.css" rel="stylesheet" type="text/css" />
    @Scripts.Render("~/bundles/jquery")
    <script src="/Scripts/jquery.json-2.3.min.js"></script>
    @RenderSection("Head", required: false)
    @Scripts.Render("~/bundles/modernizr")
    <script src="/Scripts/layout/xhr-targets.js"></script>
    <script src="/Scripts/jquery-ui-1.10.2.js" type="text/javascript"></script>
    <link href="~/Content/themes/eplat/jquery-ui-1.10.3.custom.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/ckeditor/ckeditor.js" type="text/javascript"></script>
    <script src="/Scripts/ckeditor/adapter-jquery.js" type="text/javascript"></script>
    <script src="/Scripts/ckeditor/config.js" type="text/javascript"></script>
    <script src="/Scripts/ckeditor/styles.js" type="text/javascript"></script>
    <script src="/Content/plugins/fancybox/jquery.fancybox.pack.js"></script>
    <script src="/Scripts/jquery.multiselect.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.multiselect.filter.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="/signalr/hubs"></script>
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/eplat")
    <script src="/Scripts/common.js"></script>
    <script src="/Scripts/layout/eplathub.js?v=2016-02-10"></script>
    <script src="/Scripts/layout/tickets.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue"></script>
    @*<script src="/Scripts/jstree.js" type="text/javascript"></script>*@
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-4043946-52"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-4043946-52');
    </script>

</head>
<body>
    <header id="header" class="non-printable">
        <div class="header-settings">
            <div id="headerTerminals" style="display: none">
                <img id="headerTerminalsTriangle" src="/Content/themes/base/images/triangle.png" />
                <div id="divSelectedTerminals" class="header-settings-box">Terminals</div>
                <div id="divAvailableTerminals" style="min-width: 180px;" class="header-settings-menu"></div>
            </div>
            <div id="headerWorkGroup" style="display: none">
                <img id="headerWorkGroupTriangle" src="/Content/themes/base/images/triangle.png" />
                <div id="divSelectedWorkGroup" class="header-settings-box">Work Groups</div>
                <div id="divAvailableWorkGroups" style="min-width: 145px;" class="header-settings-menu"></div>
            </div>
            <div id="headerRoles" style="display: none">
                <img id="headerRoleTriangle" src="/Content/themes/base/images/triangle.png" />
                <div id="divSelectedRole" class="header-settings-box">Roles</div>
                <div id="divAvailableRoles" style="min-width: 180px;" class="header-settings-menu">
                </div>
            </div>
            <div id="loginDisplay">
                @Html.Partial("_LogOnPartial")
            </div>
        </div>
        <div id="logo">
            ePlat
        </div>
        <nav>
            <ul id="menu"></ul>
        </nav>
    </header>
    <div id="messageBox" class="non-printable">
        <span id="messageBoxClose" class="btn-close"></span>
        <span id="messageBoxCounter"></span>
        <span id="messageBoxTitle" class="mb-confirmation"></span>
        <br />
        <span id="messageBoxContent"></span>
        <div id="messageBoxActionButtons">
            <input type="button" class="button alert-dblclick" id="messageBoxBtnConfirm" value="Confirm" />
            <input type="button" class="button alert-dblclick" id="messageBoxBtnCancel" value="Cancel" />
        </div>
        <fieldset id="messageBoxInnerException">
            <legend>
                <img class="fieldset-indicator" src="/Content/themes/base/images/eplat_show.jpg" />
                Details
            </legend>
            <div style="display: none">
                <iframe id="messageBoxDetails" frameborder="0">
                    Detalle del contenido
                </iframe>

            </div>

        </fieldset>
        <div id="messageBoxOpener">
            <span id="messageBoxOpen">
                &nbsp;
            </span>
        </div>
    </div>
    <div id="sidebar" class="non-printable">
        <div id="sidebarOpener" class="non-printable">
            <div id="sidebarTriangle" class="right-triangle"></div>
        </div>
        <div class="sidebar-pleca non-printable" id="pleca-notifications">
            <span class="notification-add">
                <span class="notification-add-text">+</span>
                <span class="notification-add-triangle"></span>
            </span>
            <span id="iconNotifications" class="notification-icon">
                <span class="notification-icon-text">!</span>
            </span>
            NOTIFICATIONS PANEL
        </div>
        <div class="sidebar-panel non-printable" id="content-notifications">
            <div id="pnlNewNotification" style="display:none;">
                <select id="ddlNotificationWorkGroup"></select>
                <textarea id="txtNotification"></textarea>
                <input type="checkbox" id="chkNotificationImportant" /> <label for="chkNotificationImportant">Important!</label>
                <input type="button" id="btnSendNotification" class="mini-button" value="SEND" />
            </div>
            <div id="pnlNotifications"></div>
        </div>
        <div class="sidebar-pleca non-printable">
            <span id="btnSupportTickets" class="tickets-list">...</span>
            <span id="pleca-tickets">
                <span id="iconTickets" class="notification-icon">
                    <span class="ticket-icon-text">:)</span>
                </span>
                MY TASKS
            </span>
        </div>
        <div class="sidebar-panel non-printable" style="height: 0" id="content-tickets">
            <h4><img id="imgUnasignedTickets" src="~/Images/loading.gif" class="right" />Unassigned Tickets</h4>
            <ul id="ulUnassignedTickets" class="connectedSortable"></ul>

            <h4><img id="imgAsignedTickets" src="~/Images/loading.gif" class="right" />My Tickets</h4>
            <ul id="ulTickets" class="connectedSortable"></ul>
            <div id="divQuickTicket">
                <input type="text" id="txtQuickSubject" placeholder="Subject" />
                <textarea id="txtQuickBody" placeholder="Details"></textarea>
                <input type="button" id="btnQuickTicket" value="Save Ticket" />
            </div>
        </div>
        <div class="sidebar-pleca non-printable" id="pleca-users">
            <span id="iconUsers" class="user-icon">
                <span class="user-icon-head"></span>
                <span class="user-icon-body"></span>
            </span>
            MY WORK GROUP
        </div>
        <div class="sidebar-panel non-printable" style="overflow: hidden" id="content-users">
            <div id="pnlSlideUsers">
                <div id="pnlUsers"></div>
                <div id="pnlChat">
                    <div class="chat-header">
                        <span class="chat-close mini-button">BACK</span>
                        <span class="chat-clear mini-button">CLEAR</span>
                        <span id="spnChattingWith"></span>
                    </div>
                    <div id="pnlConversation"></div>
                    <div id="pnlMessage">
                        <input type="hidden" id="hdnChatWith" />
                        <textarea id="txtConversation"></textarea>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="sideMenu" class="non-printable">
        <div id="sideMenuOpener" class="non-printable">
            <div id="sideMenuTriangle" class="right-triangle"></div>
            @*<div id="sideMenuTriangle" class="left-triangle"></div>*@
            @*<span>&nbsp;</span>*@
        </div>
        <div id="optionsContainer"></div>
    </div>
    <section id="main" class="page">
        @RenderBody()
    </section>
    <footer class="non-printable">@*STATS [ ONLINE USERS: <span id="spnOnlineUsers"></span> &bull; OPEN TABS: <span id="spnOpenTabs"></span> &bull; ACTIVE TABS: <span id="spnActiveTabs"></span> ]&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; TOOLS [*@ <span id="spnFooterClearCache" class="btn">CLEAR CACHE</span></footer>
    <div id="StatusBar" class="non-printable">
        <div class="message"></div>
    </div>
    @*@Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/jqueryui")
        @Scripts.Render("~/bundles/jqueryval")
        @Scripts.Render("~/bundles/eplat")*@
    <script src="/Scripts/layout/wikifields.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
