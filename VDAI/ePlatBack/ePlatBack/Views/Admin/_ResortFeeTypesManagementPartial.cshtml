@model ePlatBack.Models.ViewModels.ResortFeeTypeInfoModel

<div class="full-width align-from-bottom">
    <input type="button" id="btnNewResortFeeType" class="submit right fieldset-expander" value="new" />
</div>
<div id="divResortFeeTypes">
</div>
<div class="full-width align-from-bottom">
    <input type="button" id="btnSaveResortFeeTypesPerWorkGroup" value="save" class="submit right" />
</div>
<fieldset id="fdsResortFeeTypesInfo">
    <legend>
        <img src="/Content/themes/base/images/eplat_show.jpg" class="fieldset-indicator fds-indicator-separator" />
        Resort Fee Type Info
    </legend>
    <div class="align-from-top" style="display:none">
        @using (Ajax.BeginForm("SaveResortFeeType", "Admin", new AjaxOptions { OnSuccess = "PROFILE.saveResortFeeTypeSuccess(data)" }, new { id = "frmResortFeeType", onSubmit = "return UI.showValidationSummary(this.id);" }))
        {
            <div id="frmResortFeeType_ErrorMessages">
                @Html.ValidationMessageFor(m => m.ResortFeeTypeInfo_ResortFeeType)
            </div>
            <div class="editor-alignment">
                <div class="editor-label">
                    @Html.LabelFor(m => m.ResortFeeTypeInfo_ResortFeeType)
                    @Html.HiddenFor(m => m.ResortFeeTypeInfo_ResortFeeTypeID)
                </div>
                <div class="editor-field">
                    @Html.TextBoxFor(m => m.ResortFeeTypeInfo_ResortFeeType)
                </div>
            </div>
            <div class="full-width">
                <input type="submit" class="submit right" value="save" id="btnSaveResortFeeType" />
            </div>
        }
    </div>
</fieldset>
