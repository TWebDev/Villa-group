@model ePlatBack.Models.ViewModels.HubViewModel

@{
    //ViewBag.Title = "ePlat > Settings > Catalogs" + @Model.catalogTag;
    Layout = "~/Views/Shared/_Layout.cshtml";
    Html.EnableClientValidation(true);
}

@section Head
{
<title>CRM HUB</title>
    <link href="/Content/jquery.dataTables.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery-ui-timepicker-addon.css" rel="stylesheet" type="text/css" />
    <link href="/Content/fineuploader.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

}
@section Scripts
{
    <script src="@Url.Content("~/Scripts/jquery.json-2.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CRM/hub/hub.js")" type="text/javascript"></script>
    <script src="~/Scripts/jquery.dataTables.js" type="text/javascript"></script>
<script src="~/Scripts/ColReorder.js" type="text/javascript"></script>
<script src="~/Scripts/jquery-ui-timepicker-addon.js" type="text/javascript"></script>
<script src="/Scripts/jquery.fineuploader-3.0.min.js" type="text/javascript"></script> 
}

<h1>CRM HUB</h1>
<div class="align-from-top">
    <fieldset id="fdsGuestsManagement">
        <legend>
            <img src="/Content/themes/base/images/eplat_show.jpg" class="fieldset-indicator fds-indicator-separator" />
            SEARCH
        </legend>
        <div style="display: none" class="align-from-top">
            @Html.Partial("_SearchGuestsPartial", Model.GuestSearch)
            @Html.Partial("_GuestProfilePartial", Model.GuestInfo)
            
            @Html.Partial("_UploadHotelExpertPartial",null )
            
        </div>
    </fieldset>
    
</div>
