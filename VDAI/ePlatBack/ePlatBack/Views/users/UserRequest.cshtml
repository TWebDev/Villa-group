@model ePlatBack.Models.ViewModels.UserRequest
@{
    Layout = "~/Views/Shared/_Layout2Anonymous.cshtml";
}

@section Head {
    @if (ViewData["UserRequest"] != null)
    {
        <script type="text/javascript">
        window.UserRequestModel = JSON.parse('@Html.Raw(ViewData["UserRequest"])');
        </script>
    }
}

@section Scripts {
    <script src="~/Content/plugins/fine-uploader5.16.2/fine-uploader.min.js"></script>
    <script src="~/ViewModels/eplat.local.public.userrequest.js"></script>
}

