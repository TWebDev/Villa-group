@model ePlatBack.Models.ViewModels.CouponFoliosModel.SearchCouponFoliosModel

<fieldset id="fdsCouponFoliosManagement">
    <legend>
        <img src="/Content/themes/base/images/eplat_show.jpg" class="fieldset-indicator fds-indicator-separator" />
        Coupon Folios Management
    </legend>
    <div class="align-from-top">
        <div class="full-width">
            <input type="button" id="btnNewCouponFolioInfo" class="submit fieldset-expander right" value="new" />
        </div>
        @using (Ajax.BeginForm("SearchCouponFolios", "Catalogs", new AjaxOptions { OnSuccess = "CATALOG.couponFoliosResultsTable(data)", OnComplete = "CATALOG.makeCouponFoliosSelectable()", UpdateTargetId = "divTblExistingCouponFolios" }, new { id = "frmCouponFoliosSearch" }))
        {
            <div class="editor-alignment">
                <div class="editor-label">
                    @Html.LabelFor(model => model.SearchCouponFolios_FromFolio)
                </div>
                <div class="editor-field">
                    @Html.TextBoxFor(model => model.SearchCouponFolios_FromFolio)
                </div>
            </div>
            <div class="editor-alignment">
                <div class="editor-label">
                    @Html.LabelFor(model => model.SearchCouponFolios_ToFolio)
                </div>
                <div class="editor-field">
                    @Html.TextBoxFor(model => model.SearchCouponFolios_ToFolio)
                </div>
            </div>
            <div class="editor-alignment">
                <div class="editor-label">
                    @Html.LabelFor(model => model.SearchCouponFolios_Serial)
                </div>
                <div class="editor-field">
                    @Html.TextBoxFor(model => model.SearchCouponFolios_Serial)
                </div>
            </div>
            <div class="editor-alignment">
                <div class="editor-label">
                    @Html.LabelFor(model => model.SearchCouponFolios_Companies)
                </div>
                <div class="editor-field">
                    @Html.DropDownListFor(model => model.SearchCouponFolios_Companies, Model.SearchCouponFolios_DrpCompanies, new { @class = "terminal-dependent-list", data_route = "/TimeShare/GetDDLData", data_route_parameter = "companiesPerSelectedTerminals", multiple = "multiple" })
                </div>
            </div>
            <div class="editor-alignment align-from-top">
                <div class="editor-field">
                    <input type="submit" class="button search" value="search" />
                </div>
            </div>
            <div id="divTblExistingCouponFolios" class="align-from-top">
                <table id="tblSearchCouponFoliosResults" class="dataTable table search-results">
                    <thead>
                        <tr>
                            <th>From Folio</th>
                            <th>To Folio</th>
                            <th>Serial</th>
                            <th>Delivered</th>
                            <th>Generated</th>
                            <th>Available</th>
                            <th>Company</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        }
        @Html.Action("RenderCouponFoliosManagement", "Catalogs")
    </div>
</fieldset>