@model ePlatBack.Models.ViewModels.CatalogsViewModel

@{
    ViewBag.Title = "ePlat > Settings > Catalogs" + @Model.catalogTag;
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Privileges = Model.Privileges;
    ViewData["Privileges"] = Model.Privileges;
    Html.EnableClientValidation(true);
}

@section Head{
    <link href="~/Content/jquery-ui-1.8.14.custom.css" rel="stylesheet" type="text/css" />
    <link href="/Content/jquery.dataTables.css" rel="stylesheet" type="text/css" />
    <link href="/Content/fineuploader.css" rel="stylesheet" type="text/css" />
}
@section Scripts{
    <script src="/Scripts/Common.js" type="text/javascript"></script>
    <script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=false"></script>
    <script src="~/Scripts/jquery.json-2.3.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.js" type="text/javascript"></script>
    @*<script src="/Scripts/jquery.fineuploader-3.0.js" type="text/javascript"></script>*@
    <script src="/Scripts/jquery.fineuploader-3.0.min.js" type="text/javascript"></script>

    <script src="~/Scripts/settings/catalogs/catalogs.js?v=@DateTime.Now" type="text/javascript"></script>
    <script src="~/Scripts/cms/seoItems.js" type="text/javascript"></script>
}
<h1>@Html.DisplayFor(model => model.catalogTag)</h1>

@Html.Action("RenderCatalogs", "Catalogs", new { catalogName = Model.catalogName })
